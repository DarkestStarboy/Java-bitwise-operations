import java.util.*;
public class Main{
    public static void main(String args[]){
        byte a=0b00001010;
        byte b=0b00000101;
        
       int and= a&b;
       System.out.println(String.format("%8s",Integer.toBinaryString(and)));
       
       int or= a|b;
        System.out.println(String.format("%8s",Integer.toBinaryString(or)));
       
       int xor= a^b;
        System.out.println(String.format("%8s",Integer.toBinaryString(xor)));
        
       int notA= ~a;
        System.out.println(String.format("%s",Integer.toBinaryString(notA)));
       
       int notB= ~b;
        System.out.println(String.format("%s",Integer.toBinaryString(notB)));
     
      int leftShift=  a<<1;
       System.out.println(String.format("%8s",Integer.toBinaryString(leftShift)));
       
      int rightShift=  b>>1;
       System.out.println(String.format("%8s",Integer.toBinaryString(rightShift)));
       
      int unSignedRightShift=  a>>>1;
       System.out.println(String.format("%8s",Integer.toBinaryString(unSignedRightShift)));
      
    }
}
