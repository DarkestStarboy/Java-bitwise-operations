import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
String num=sc.nextLine();
System.out.println(num);

System.out.println("the given string is binary "+ num.matches("[0-1]*"));
System.out.println("the given string is hexa decimal "+num.matches("A-F0-9*"));
System.out.println("the given string is octal "+num.matches("[1-8]*"));
System.out.println("the given string is decimal "+num.matches("[0-9]*"));
    }
}
