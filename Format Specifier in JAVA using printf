import java.util.*;
public class Main{
    public static void main(String args[]){
        
        int a=12;
        float f=1.2f;
        String str = "hello";
        
        System.out.println(a +" " + f +" "+ str);
        
        System.out.printf("%d %f %s \n",a,f,str);
        System.out.printf("%2$d %3$f %1$s \n",str,a,f);
    }
}
