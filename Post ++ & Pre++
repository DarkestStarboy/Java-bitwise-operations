import java.util.*;
public class Main{
    public static void main(String args[]){
        
        int a=5;
        int b;
        b=a++;
        System.out.println("b = " +b+ " a = "+a);
        
        int  y=5;
        int z;
        z=++y;
        System.out.println("y = " +y+" z = "+z);
    }
}
